{
    "description": "Rclone plugin for Docker",
    "documentation": "https://docs.docker.com/engine/extend/plugins/",
    "entrypoint": [
        "/usr/bin/docker-volume-rclone",
        "daemon"
    ],
    "env": [
        {
            "name": "DEBUG",
            "settable": [
                "value"
            ],
            "value": "0"
        }
    ],
    "interface": {
        "socket": "rclone.sock",
        "types": [
            "docker.volumedriver/1.0"
        ]
    },
    "mounts": [
        {
          "name": "cache",
          "description": "Cache folder to use for cache mount type",
          "destination": "/var/cache/rclone",
          "source": "/var/cache/rclone",
          "settable": [
            "source",
            "destination"
          ],
          "type": "bind"
        }
    ],
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "devices": [
            {
                "path": "/dev/fuse"
            }
        ]
    },
    "network": {
        "type": "host"
    },
    "propagatedmount": "/var/lib/docker-volumes/rclone"
}
